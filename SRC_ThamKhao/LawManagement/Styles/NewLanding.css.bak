.ImageIcon {
    margin-bottom: 12px;
    height: 100px;
    width: 100px;   
    border: 0px;
    border-style: none;
    border-spacing: inherit;
    border-collapse: inherit;
    -moz-border-radius: 7px;
    -webkit-border-radius: 7px;
    border-radius: 10%;
    background-size: 100px
}

    .ImageIcon:not(.GrayOut) {
        cursor: pointer;
        background-color: #0c547a;
    }

    .ImageIcon.GrayOut {
        cursor: default;
        background-color: #808080;
    }
input[type=button].ImageIcon:not(.GrayOut):hover {   
    background-color: #007aaa
}
